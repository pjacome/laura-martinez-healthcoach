    .myTextarea {
    }

    .container {
        padding-top: 40px;
        padding-left: 40px;
        padding-right: 40px;
    }

    .allcaps {
        font-variant: small-caps;
    }

    .bold h5 {
        font-weight: bold !important;
    }

    .underline {
        text-decoration: underline;
    }

    .inner-title {
        color: red;
    }

    #saveButton,
    #printButton {
        margin-top: 20px;
        margin-right: 10px;
    }

