        </form>
    </div>
    <div class="hide-on-med-and-up center">
        <div class="flow-text">Subscribe to my email list to get updates on recipes, blogs, events and more!</div>
        <br>
        <button id="blog-subscribe-btn-sm" class="btn btn-flat waves-effect waves-light orange-text grey lighten-3">Subscribe!</button>
        <br>
        <br>
    </div>
    <!-- end -->
    <br>
    <div class="tile-wrapper">
        <div class="tile-table">
            <div class="tile-table-row">

                <!-- Template -->
                {{#each blogResult}}
                <div class="single-tile">
                    <a href="blogs/{{_id}}">
                        <div class="img-wrapper"><img src="images/uploads/{{sections.[0]}}"></div>
                        <span class="flow-text">{{title}}</span>
                    </a>
                </div>
                {{/each}}

            </div>
        </div>
    </div>
</div>

<style type="text/css">
    .subscribe-wrapper.lg .input-field {
        display: inline-block;
    }

    div.social-icons {
        margin-bottom: 5px;
    }
    
    /* logo */
    .lmhc-logo-wrapper {
        width: 100%;
        height: 100px;
    }

    .logo-table { display: table; width: inherit; height: inherit; border-bottom: 1px black solid; }
    .logo-table-row { display: table-row; width: inherit; height: inherit; }
    .logo-table-cell { display: table-cell; width: inherit; height: inherit; }
    .logo-table-cell.left { width: 100px; }
    .logo-table-cell.left > img { width: 100px; height: 100px; }

    .logo-table-cell.right {
        width: calc(100% - 100px);
        font-size: 3rem;
        text-align: center;
        position: relative;
    }

    .logo-table-cell.right > div {
        position: absolute;
        width: 100%;
        top: 50%;
        left: 50%;
        transform: translate(-50%, -50%);
        -webkit-transform: translate(-50%, -50%);
        -moz-transform: translate(-50%, -50%);
        -ms-transform: translate(-50%, -50%);
    }
    /* logo - end */

    /* tiles */
    .tile-wrapper { width: 100%; }
    .tile-table { width: 100%; display: table; }
    .tile-table-row { width: 100%; display: table-row; }
    .tile-table-row .single-tile { 
        width: 50%; max-width: 50%;
        display: inline-block;
        margin-bottom: 10px;
        vertical-align: top;
        padding: 5px;
    }
    
    .single-tile .img-wrapper {
        width: 100%;
        height: 200px;
        max-height: 200px;
        overflow: hidden;
        margin: 0 auto;
        padding: 5px;
    }

    .single-tile span { text-align: center; display: block; color: black; }

    .single-tile .img-wrapper img {
        width: 100%;
        height: auto;
        image-rendering: pixelated;
    }
    /* tiles - end */

    /* mobile: landscape */
    @media (max-width: 768px) and (orientation: landscape) {
        .subscribe-form.lg {
            height: 100%;
            margin: 0 auto;
            padding: 10px 5px 0px 5px;
            border-radius: 3px;
            background-color: #f7f7f7;
        }

        .subscribe-wrapper.lg .row {
            margin-bottom: 0;
        }

        .subscribe-form.lg .text {
            font-variant-caps: small-caps;
            font-size: 1.3em;
        }

        .subscribe-form.lg input {
            border-bottom: 1px black solid;
        }

        /* logo */
        .lmhc-logo-wrapper { height: 150px; }
        .logo-table-cell.left { width: 150px; }
        .logo-table-cell.left > img { width: 150px; height: 150px; }
        .logo-table-cell.right { width: calc(100% - 150px); }
        /* logo - end */

        /* tiles */
        .tile-table-row .single-tile { 
            width: 33%; max-width: 33%;
        }
        /* tiles - end */
    }

    /* small */
    @media (min-width: 768px) {
        .subscribe-form.lg {
            height: 100%;
            margin: 0 auto;
            padding: 10px 5px 0px 5px;
            border-radius: 3px;
            background-color: #f7f7f7;
        }

        .subscribe-wrapper.lg .row {
            margin-bottom: 0;
        }

        .subscribe-form.lg .text {
            font-variant-caps: small-caps;
            font-size: 1.5em;
        }

        .subscribe-form.lg input {
            border-bottom: 1px black solid;
        }

        /* logo */
        .lmhc-logo-wrapper { height: 150px; }
        .logo-table-cell.left { width: 150px; }
        .logo-table-cell.left > img { width: 150px; height: 150px; }
        .logo-table-cell.right { width: calc(100% - 150px); }

        .logo-table-cell.right {
            width: calc(100% - 150px);
            font-size: 4rem;
        }
        /* logo - end */
    }

    /* medium */
    @media (min-width: 992px) {
    }

    /* desktop */
    @media (min-width: 1200px) {
        .tile-table-row .single-tile { 
            width: 20%; max-width: 20%;
        }
    }

    /* helper classes */
</style>

<script type="text/javascript">
    $(document).ready(function() {
        //
    });

    /*
        Waits for all images to load
        then adjusts the height of each tile
    */
    $(window).load(function() {
        var array = $('.tile-table-row').children('.single-tile');
        $.each(array, function(index, value) {
            console.log($(value).find('img'));
            var target = $(value).find('img')[0];
            var imgHeight = $(target).css('height');
            var wrapper = $(value).find('.img-wrapper');
            imgHeight = parseInt(imgHeight.split('px')[0]);
            imgHeight += 10;
            console.log(imgHeight);
            $(wrapper).css('height', imgHeight + 'px');
        });
    });
</script>